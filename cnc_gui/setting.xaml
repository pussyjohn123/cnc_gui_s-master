                                </ControlTemplate>
                            </TextBox.Template>
                        </hc:TextBox>
                        <hc:TextBox x:Name="flusher_address_st" Width="100" Height="35" FontSize="20" Text=" " BorderThickness="0" Padding="0" Background="Black" Margin="10,0,0,0" TextChanged="excluder_period_st_TextChanged" KeyDown="excluder_period_st_KeyDown">
                            <TextBox.Template>
                                <ControlTemplate TargetType="TextBox">
                                    <Border Background="#c0c0c0" BorderBrush="Black" BorderThickness="1" CornerRadius="4">
                                        <ScrollViewer x:Name="PART_ContentHost"/>
                                    </Border>
                                </ControlTemplate>
                            </TextBox.Template>
                        </hc:TextBox>
                        <hc:TextBox x:Name="flusher_address_binary_st" Width="200" Height="35" FontSize="20" Text=" " BorderThickness="0" Padding="0" Background="Black" Margin="20,0,0,0" TextChanged="excluder_period_st_TextChanged" KeyDown="excluder_period_st_KeyDown">
                            <TextBox.Template>
                                <ControlTemplate TargetType="TextBox">
                                    <Border Background="#c0c0c0" BorderBrush="Black" BorderThickness="1" CornerRadius="4">
                                        <ScrollViewer x:Name="PART_ContentHost"/>
                                    </Border>
                                </ControlTemplate>
                            </TextBox.Template>
                        </hc:TextBox>
                        <hc:TextBox x:Name="flusher_address_decimal_st" Width="100" Height="35" FontSize="20" Text=" " BorderThickness="0" Padding="0" Background="Black" Margin="20,0,0,0" TextChanged="excluder_period_st_TextChanged" KeyDown="excluder_period_st_KeyDown">
                            <TextBox.Template>
                                <ControlTemplate TargetType="TextBox">
                                    <Border Background="#c0c0c0" BorderBrush="Black" BorderThickness="1" CornerRadius="4">
                                        <ScrollViewer x:Name="PART_ContentHost"/>
                                    </Border>
                                </ControlTemplate>
                            </TextBox.Template>
                        </hc:TextBox>
                        <Button x:Name="address_save" Style="{StaticResource ButtonPrimary}" FontSize="15" Content="寫入點位" Height="35" Margin="10,0,0,0" Click="address_save_Click"/>
                    </StackPanel>
                </StackPanel>
                <StackPanel Orientation="Horizontal" Margin="15,15,0,0">
                    <TextBlock Text="  排屑機  " FontSize="20" Margin="0,5,0,0"/>
                    <hc:TextBox x:Name="excluder_address_handl_st" Width="22" Height="35" FontSize="20" Text=" " BorderThickness="0" Padding="0" Background="Black" Margin="35,0,0,0" TextChanged="excluder_period_st_TextChanged" KeyDown="excluder_period_st_KeyDown">
                        <TextBox.Template>
                            <ControlTemplate TargetType="TextBox">
                                <Border Background="#c0c0c0" BorderBrush="Black" BorderThickness="1" CornerRadius="4">
                                    <ScrollViewer x:Name="PART_ContentHost"/>
                                </Border>
                            </ControlTemplate>
                        </TextBox.Template>
                    </hc:TextBox>
                    <hc:TextBox x:Name="excluder_address_st" Width="100" Height="35" FontSize="20" Text=" " BorderThickness="0" Padding="0" Background="Black" Margin="10,0,0,0" TextChanged="excluder_period_st_TextChanged" KeyDown="excluder_period_st_KeyDown">
                        <TextBox.Template>
                            <ControlTemplate TargetType="TextBox">
                                <Border Background="#c0c0c0" BorderBrush="Black" BorderThickness="1" CornerRadius="4">
                                    <ScrollViewer x:Name="PART_ContentHost"/>
                                </Border>
                            </ControlTemplate>
                        </TextBox.Template>
                    </hc:TextBox>
                    <hc:TextBox x:Name="excluder_address_binary_st" Width="200" Height="35" FontSize="20" Text=" " BorderThickness="0" Padding="0" Background="Black" Margin="20,0,0,0" TextChanged="excluder_period_st_TextChanged" KeyDown="excluder_period_st_KeyDown">
                        <TextBox.Template>
                            <ControlTemplate TargetType="TextBox">
                                <Border Background="#c0c0c0" BorderBrush="Black" BorderThickness="1" CornerRadius="4">
                                    <ScrollViewer x:Name="PART_ContentHost"/>
                                </Border>
                            </ControlTemplate>
                        </TextBox.Template>
                    </hc:TextBox>
                    <hc:TextBox x:Name="excluder_address_decimal_st" Width="100" Height="35" FontSize="20" Text=" " BorderThickness="0" Padding="0" Background="Black" Margin="20,0,0,0" TextChanged="excluder_period_st_TextChanged" KeyDown="excluder_period_st_KeyDown">
                        <TextBox.Template>
                            <ControlTemplate TargetType="TextBox">
                                <Border Background="#c0c0c0" BorderBrush="Black" BorderThickness="1" CornerRadius="4">
                                    <ScrollViewer x:Name="PART_ContentHost"/>
                                </Border>
                            </ControlTemplate>
                        </TextBox.Template>
                    </hc:TextBox>
                    <Button x:Name="address_load" Style="{StaticResource ButtonPrimary}" FontSize="15" Content="讀取點位" Height="35" Margin="10,0,0,0" Click="address_load_Click"/>
                </StackPanel>
            </StackPanel>
        </Border>
        <Border Width="735" Height="260" CornerRadius="30" Margin="340,224,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Background="White" Grid.RowSpan="4" Grid.ColumnSpan="3">
            <StackPanel Margin="0,10,0,0">
                <TextBlock Text="底座環沖執行時間設定" FontSize="30" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,10,0,0"/>
                <StackPanel Orientation="Horizontal">
                    <lvc:PieChart Series="{Binding pie_Flusher_time_total}" Margin="20,0,0,0" Height="180" Width="200" />
                    <StackPanel Margin="100,0,0,0">
                        <StackPanel Orientation="Horizontal" Margin="0,30,0,0">
                            <TextBlock  Text="&#xf111;" FontSize="22" Foreground="#42A5F4" Margin="0,0,5,0" FontFamily="/font/Font Awesome 6 Free Solid.otf#Font Awesome 6 Free Solid" Height="25"/>
                            <TextBlock Text="影像擷取與計算時間" FontSize="20"/>
                            <hc:TextBox x:Name="image_processing_time" Width="70" Height=" 30" FontSize="20" VerticalContentAlignment="Center" Background="Black" Margin="10,0,0,0" TextChanged="image_processing_time_TextChanged" KeyDown="image_processing_time_KeyDown">
