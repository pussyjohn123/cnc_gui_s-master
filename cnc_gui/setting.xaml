                        <hc:TextBox x:Name="flusher_address_handl_st"  Width="22" Height="35" FontSize="20" Text=" " BorderThickness="0" Padding="0" Background="Black" Margin="35,0,0,0" TextChanged="excluder_period_st_TextChanged" KeyDown="excluder_period_st_KeyDown">
                            <TextBox.Template>
                                <ControlTemplate TargetType="TextBox">
                                    <Border Background="#c0c0c0" BorderBrush="Black" BorderThickness="1" CornerRadius="4">
                                        <ScrollViewer x:Name="PART_ContentHost"/>
