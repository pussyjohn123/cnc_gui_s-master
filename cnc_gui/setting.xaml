                            <TextBlock  Text="&#xf111;" FontSize="22" Foreground="#F55F54" Margin="0,0,5,0" FontFamily="/font/Font Awesome 6 Free Solid.otf#Font Awesome 6 Free Solid" Height="25"/>
                            <TextBlock Text="可沖水總時間            " FontSize="20"/>
                            <hc:TextBox x:Name="total_flusher_time" Width="70" Height=" 30" FontSize="20" VerticalContentAlignment="Center" Background="Black" Margin="10,0,0,0" TextChanged="total_flusher_time_TextChanged" KeyDown="total_flusher_time_KeyDown">
                                <TextBox.Template>
                                    <ControlTemplate TargetType="TextBox">
                                        <Border Background="#c0c0c0" BorderBrush="Black" BorderThickness="1" CornerRadius="4">
                                            <ScrollViewer x:Name="PART_ContentHost"/>
                                        </Border>
                                    </ControlTemplate>
                                </TextBox.Template>
                            </hc:TextBox>
                            <TextBlock Text="秒" FontSize="20" Margin="10,0,0,0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="0,10,0,0">
                            <TextBlock  Text="&#xf111;" FontSize="22" Foreground="#F1C92C" Margin="0,0,5,0" FontFamily="/font/Font Awesome 6 Free Solid.otf#Font Awesome 6 Free Solid" Height="25"/>
                            <TextBlock Text="保留時間                    " FontSize="20"/>
